class Solution {
public:
    int makeTheIntegerZero(int num1, int num2) {
        for (int k = 1; k <= 60; ++k) {
            long long S = (long long)num1 - (long long)k * num2;
            if (S < k) continue;
            int bitcount = __builtin_popcountll(S);
            if (bitcount <= k) return k;
            }
        return -1;
    }
};
